<h2>Signin</h2>

<%= form_tag login_path, method: :post, html: {novalidate: "novalidate"} do %>
<p>
<%= label_tag :email %><br>
<%= email_field_tag :email %>
</p>
<p>
<%= label_tag :password %><br>
<%= password_field_tag :password %>
</p>
<p>
<%= submit_tag "Signin" %>
</p>
<% end %>

<%= link_to "New User?", signup_path %>
